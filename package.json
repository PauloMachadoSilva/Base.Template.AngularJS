{
    "name": "base-template-angularjs",
    "version": "6.1.1",
    "scripts": {
        "build:dev": "webpack --config webpack/webpack.config.dev.js",
        "build:hmg": "webpack --config webpack/webpack.config.hmg.js",
        "build:hmgInterno": "webpack --config webpack/webpack.config.hmgInterno.js",
        "build:prod": "npm run coverage && webpack -p --config webpack/webpack.config.prod.js",
        "coverage": "karma start --single-run",
        "lint": "./node_modules/.bin/eslint src/**/*.js",
        "precommit": "npm run lint",
        "prepublishOnly": "npm run build",
        "prepush": "npm run coverage",
        "start:hmg": "webpack-dev-server --port=3000 --config webpack/webpack.config.hmg.js --open",
        "start:hmgInterno": "webpack-dev-server --port=3000 --config webpack/webpack.config.hmgInterno.js --open",
        "start:prod": "webpack-dev-server --port=3000 --config webpack/webpack.config.prod.js --open --mode production",
        "start": "webpack-dev-server --port=3000 --config webpack/webpack.config.dev.js --open --mode development",
        "test": "karma start"
    },
